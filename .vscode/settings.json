{
    "Lua.workspace.library": [
        "${addons}/starfallex/module/library"
    ],
    "Lua.runtime.plugin": "${addons}/starfallex/module/plugin.lua",
    "Lua.runtime.version": "LuaJIT",
    "Lua.runtime.builtin": {
        "basic": "disable",
        "coroutine": "disable",
        "string": "disable",
        "table": "disable",
        "math": "disable",
        "io": "disable",
        "os": "disable",
        "debug": "disable",
        "bit": "disable",
        "jit": "disable",
        "utf8": "disable",
        "package": "disable",
        "bit32": "disable",
        "ffi": "disable",
        "builtin": "enable",
        "jit.profile": "disable",
        "jit.util": "disable",
        "string.buffer": "disable",
        "table.clear": "disable",
        "table.new": "disable"
    },
    "Lua.runtime.nonstandardSymbol": [
        "/**/"
    ],
    "Lua.diagnostics.disable": [
        "duplicate-set-field"
    ],
    "Lua.workspace.checkThirdParty": false
}